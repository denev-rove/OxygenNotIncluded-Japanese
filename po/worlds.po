msgctxt "STRINGS.WORLDS.SURVIVAL_CHANCE.LEASTHOSPITABLE"
msgid "Inhospitable"
msgstr "人を寄せ付けない"

#. STRINGS.WORLDS.SURVIVAL_CHANCE.LOW
msgctxt "STRINGS.WORLDS.SURVIVAL_CHANCE.LOW"
msgid "Marginal"
msgstr "ぎりぎり"

